{
  "name": "jekyll-post-creator",
  "displayName": "Jekyll Post Creator",
  "description": "Create Jekyll post markdown files with proper frontmatter formatting",
  "version": "1.0.0",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jekyll-post-creator.createPost",
        "title": "Jekyll: Create New Post"
      },
      {
        "command": "jekyll-post-creator.showCreatorWindow",
        "title": "Jekyll: Show Creator Window"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "jekyll-post-creator.createPost",
          "group": "jekyll"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "homepage": "https://github.com/nativvstudios/jekyll-post-creator#readme",
  "bugs": {
    "url": "https://github.com/nativvstudios/jekyll-post-creator/issues"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^8.40.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nativvstudios/jekyll-post-creator"
  },
  "keywords": [
    "jekyll",
    "blog",
    "markdown",
    "post"
  ],
  "publisher": "nativvstudios"
}
